# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore all site specific data
/web/sites/*
!/web/sites/default/
!/web/sites/example.sites.php
/vendor/

# Ignore IDE files
/.idea/
/.vscode/
*.iml
*.code-workspace

# Ignore all log files
*.log

# Ignore node modules
node_modules

# Ignore some css related stuff
*.css.map
*.css.map.map

# Ignore default text files
/CHANGELOG.txt
/COPYRIGHT.txt
/INSTALL*.txt
/LICENSE.txt
/MAINTAINERS.txt
/UPGRADE.txt
/README.txt
/web/sites/README.txt
/web/sites/all/modules/README.txt
/web/sites/all/themes/README.txt
/web/sites/*/settings*.php
/web/sites/*/settings*.yml
/web/sites/*/files
/web/sites/*/private
